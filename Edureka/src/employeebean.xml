<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:context = "http://www.springframework.org/schema/context"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans     
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context 
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">

   <context:component-scan base-package = "com.edureka" />

	<bean id="emp1" class="com.edureka.Employee">
		<property name="eid" value="102" />
		<property name="ename" value="Anvesh" />
		<property name="eaddress" value="Lake Terrace" />
	</bean>
	
	<bean id="emp2" class="com.edureka.Employee">
		<property name="eid" value="103" />
		<property name="ename" value="Harshith" />
		<property name="eaddress" value="Houston" />
	</bean>

</beans>